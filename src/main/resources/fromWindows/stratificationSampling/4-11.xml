    <unit>人</unit>  <!--默认值是“null”-->
  </TS>  
  <SOT>  <!--Stratification of TotalSet表示总体中的层-->
    <name>A类学校</name>  
    <quantity>4000</quantity>  
    <unit>人</unit> 
  </SOT>  
  <SOT> 
    <name>B类学校</name>  
    <quantity>2000</quantity>  
    <unit>人</unit> 
  </SOT>  
  <SOT> 
    <name>C类学校</name>  
    <quantity>3000</quantity>  
    <unit>人</unit> 
  </SOT>  
  <S> <!--Sample表示样本-->
    <name>样本</name>  <!--默认值是“样本”-->
    <quantity>900</quantity>  
    <unit>人</unit> 
  </S>  
  <SOS> <!--Stratification of Sample表示样本中的层-->
    <name>A类学校</name>  
    <quantity>null</quantity>  
    <unit>人</unit> 
  </SOS>  
  <SOS> 
    <name>B类学校</name>  
    <quantity>null</quantity>  
    <unit>人</unit> 
  </SOS>
  <SOS> 
    <name>C类学校</name>  
    <quantity>null</quantity>  
    <unit>人</unit> 
  </SOS>  
