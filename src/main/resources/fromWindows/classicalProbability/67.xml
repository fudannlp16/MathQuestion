<?xml version="1.0" encoding="utf-8"?>

<ClassicalProbability> 
  <Experiment> 
    <!--试验-->  
    <ExperimentObject> 
      <!--试验对象-->  
      <ObjectName>集合</ObjectName>  
      <!--对象名称-->  
      <ObjectTotality>9</ObjectTotality>  
      <!--对象总数，如果未知，用n表示-->  
        <DiscreteName>1;2;3;4;5;6;7;8;9</DiscreteName> 
        <DomainQuantity>1;1;1;1;1;1;1;1;1</DomainQuantity>  <!--默认值均是1-->
    </ExperimentObject>  
     <ExperimentMethod> 
        <!--试验方法-->  
        <SamplingWay>1</SamplingWay><!--抽取方式：放回(0)与不放回(1)，默认值是1-->  
        <SamplingQuantity>2</SamplingQuantity>          <!--抽取的数量，默认值1-->  
        <SortingWay>1</SortingWay> <!--排序方式：有序(0)和无序(1)，默认值是1--> 
      </ExperimentMethod>  
    <ExperimentTimes>1</ExperimentTimes><!--试验次数，默认值为1-->
    <ExperimentResult>  
        <DiscreteName>null</DiscreteName>  
        <DomainQuantity>null</DomainQuantity>  <!--默认值均是1-->
    </ExperimentResult>     
  </Experiment>
  <Question>
    <EventName>其中一个数恰是另一个数的3倍的概率</EventName>
    <ResultFilter>或(倍数(x1,x2)=3,倍数(x2,x1)=3)</ResultFilter>
    <Probability>null</Probability>
  </Question>
</ClassicalProbability>

<!--
从集合{1,2,3,4,5,6,7,8,9}中任取两个不同的数，则其中一个数恰是另一个数的3倍的概率为(  )
-->