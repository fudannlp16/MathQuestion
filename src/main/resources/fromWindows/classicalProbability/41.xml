<?xml version="1.0" encoding="utf-8"?>

<ClassicalProbability> 
  <Experiment> 
    <!--试验-->  
    <ExperimentObject> 
      <!--试验对象-->  
      <ObjectName>骰子</ObjectName>  
      <!--对象名称-->  
      <ObjectTotality>6</ObjectTotality>  
      <!--对象总数，如果未知，用n表示-->  
        <DiscreteName>1;2;3;4;5;6</DiscreteName> 
        <DomainQuantity>1;1;1;1;1;1</DomainQuantity>  <!--默认值均是1-->
    </ExperimentObject>  
     <ExperimentMethod> 
        <!--试验方法-->  
        <SamplingWay>1</SamplingWay><!--抽取方式：放回(0)与不放回(1)，默认值是1-->  
        <SamplingQuantity>1</SamplingQuantity>          <!--抽取的数量，默认值1-->  
        <SortingWay>1</SortingWay> <!--排序方式：有序(0)和无序(1)，默认值是1--> 
      </ExperimentMethod>  
    <ExperimentTimes>1</ExperimentTimes><!--试验次数，默认值为1-->
    <ExperimentResult>  
        <DiscreteName>4</DiscreteName>  
        <DomainQuantity>1</DomainQuantity>  <!--默认值均是1-->
    </ExperimentResult>     
  </Experiment>
  <Question>
    <EventName>向上的数是4的概率</EventName>
    <ResultFilter>null</ResultFilter>
    <Probability>null</Probability>
  </Question>
</ClassicalProbability>

<!--
向桌面掷骰子1次，则向上的数是4的概率是( )
-->