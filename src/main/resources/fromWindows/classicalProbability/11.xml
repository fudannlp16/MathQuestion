<?xml version="1.0" encoding="utf-8"?>

<ClassicalProbability> 
  <Experiment> 
    <!--试验-->  
    <ExperimentObject> 
      <!--试验对象-->  
      <ObjectName>球</ObjectName>  
      <!--对象名称-->  
      <ObjectTotality>5</ObjectTotality>  
      <!--对象总数，如果未知，用n表示-->  
        <DiscreteName>1;2;3;4;5</DiscreteName> 
        <DomainQuantity>1;1;1;1;1</DomainQuantity>  <!--默认值均是1-->
    </ExperimentObject>  
     <ExperimentMethod> 
        <!--试验方法-->  
        <SamplingWay>0</SamplingWay><!--抽取方式：放回(0)与不放回(1)，默认值是1-->  
        <SamplingQuantity>1</SamplingQuantity>          <!--抽取的数量，默认值1-->  
        <SortingWay>1</SortingWay> <!--排序方式：有序(0)和无序(1)，默认值是1--> 
      </ExperimentMethod>  
    <ExperimentTimes>2</ExperimentTimes><!--试验次数，默认值为1-->
    <ExperimentResult>  
        <DiscreteName>null</DiscreteName>  
        <DomainQuantity>null</DomainQuantity>  <!--默认值均是1-->
    </ExperimentResult>     
  </Experiment>
  <Question>
    <EventName>取出的2个球中最大数字为5的概率</EventName>
    <ResultFilter>最大(x1,x2)=5</ResultFilter>
    <Probability>null</Probability>
  </Question>
</ClassicalProbability>

<!--
一袋中装有分别标记着1，2，3，4，5数字的5个球，
从袋中每次取出一个球，取出后放回，连续取2次，试求取出的2个球中最大数字为5的概率
-->