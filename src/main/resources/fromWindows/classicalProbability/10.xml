<?xml version="1.0" encoding="utf-8"?>

<ClassicalProbability> 
  <Experiment> 
    <!--试验-->  
    <ExperimentObject> 
      <!--试验对象-->  
      <ObjectName>社区</ObjectName>  
      <!--对象名称-->  
      <ObjectTotality>3</ObjectTotality>  
      <!--对象总数，如果未知，用n表示-->  
        <DiscreteName>null</DiscreteName> 
        <DomainQuantity>1;1;1</DomainQuantity>  <!--默认值均是1-->
    </ExperimentObject>  
     <ExperimentMethod> 
        <!--试验方法-->  
        <SamplingWay>0</SamplingWay><!--抽取方式：放回(0)与不放回(1)，默认值是1-->  
        <SamplingQuantity>1</SamplingQuantity>          <!--抽取的数量，默认值1-->  
        <SortingWay>1</SortingWay> <!--排序方式：有序(0)和无序(1)，默认值是1--> 
      </ExperimentMethod>  
    <ExperimentTimes>4</ExperimentTimes><!--试验次数，默认值为1-->
    <ExperimentResult>  
        <DiscreteName>null</DiscreteName>  
        <DomainQuantity>null</DomainQuantity>  <!--默认值均是1-->
    </ExperimentResult>     
  </Experiment>
  <Question>
    <EventName>每个社区至少一名义工，则甲、乙两人被分到不同社区的概率</EventName>
    <ResultFilter>至少(x1)=1;不同(甲,乙)</ResultFilter>
    <Probability>null</Probability>
  </Question>
</ClassicalProbability>

