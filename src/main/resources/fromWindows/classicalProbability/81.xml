<?xml version="1.0" encoding="utf-8"?>

<ClassicalProbability> 
  <Experiment> 
    <!--试验-->  
    <ExperimentObject> 
      <!--试验对象-->  
      <ObjectName>汤圆</ObjectName>  
      <!--对象名称-->  
      <ObjectTotality>null</ObjectTotality>  
      <!--对象总数，如果未知，用n表示-->  
        <DiscreteName>芝麻馅汤圆;花生馅汤圆;豆沙馅汤圆</DiscreteName>  
        <!--离散值域的名称-->  
        <DomainQuantity>6;5;4</DomainQuantity>  
        <!--离散值的数量，默认值为1-->  
    </ExperimentObject>  
      <ExperimentMethod> 
        <SamplingWay>1</SamplingWay>  <!--抽取方式：放回(0)与不放回(1)，默认值是1-->  
        <SamplingQuantity>4</SamplingQuantity>  
        <!--抽取的数量-->  
        <SortingWay>1</SortingWay>  <!--排序方式：有序(0)和无序(1)，默认值是1--> 
      </ExperimentMethod>   
    <ExperimentTimes>1</ExperimentTimes>  <!--试验次数，默认值为1-->
    <ExperimentResult> 
        <DiscreteName>null</DiscreteName>   
        <DomainQuantity>null</DomainQuantity><!--实验结果离散值的数量，默认值为1--> 
    </ExperimentResult>
  </Experiment>
  <Question>
    <EventName>每种汤圆都至少取到1个的概率</EventName>
    <ResultFilter>
    	至少(每种(x1,x2,x3,x4))=1
    </ResultFilter>
    <Probability>null</Probability>
  </Question>
</ClassicalProbability>

<!--
锅中煮有芝麻馅汤圆6个，花生馅汤圆5个，豆沙馅汤圆4个，这三种汤圆的外部特征完全相同。从中任意舀取4个汤圆，则每种汤圆都至少取到1个的概率为(  )
-->