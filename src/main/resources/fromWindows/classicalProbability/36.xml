<?xml version="1.0" encoding="utf-8"?>

<ClassicalProbability> 
  <Experiment> 
    <!--试验-->  
    <ExperimentObject> 
      <!--试验对象-->  
      <ObjectName>数字</ObjectName>  
      <!--对象名称-->  
      <ObjectTotality>4</ObjectTotality>  
      <!--对象总数，如果未知，用n表示-->  
        <DiscreteName>1;2;3;4</DiscreteName> 
        <DomainQuantity>1;1;1;1</DomainQuantity>  <!--默认值均是1-->
    </ExperimentObject>  
     <ExperimentMethod> 
        <!--试验方法-->  
        <SamplingWay>1</SamplingWay><!--抽取方式：放回(0)与不放回(1)，默认值是1-->  
        <SamplingQuantity>2</SamplingQuantity>          <!--抽取的数量，默认值1-->  
        <SortingWay>1</SortingWay> <!--排序方式：有序(0)和无序(1)，默认值是1--> 
      </ExperimentMethod>  
    <ExperimentTimes>1</ExperimentTimes><!--试验次数，默认值为1-->
    <ExperimentResult>  
        <DiscreteName>null</DiscreteName>  
        <DomainQuantity>null</DomainQuantity>  <!--默认值均是1-->
    </ExperimentResult>     
  </Experiment>
  <Question>
    <EventName>取两个不同的数，其和大于积的概率</EventName>
    <ResultFilter>大于(和(x1,x2),积(x1,x2))</ResultFilter>
    <Probability>null</Probability>
  </Question>
</ClassicalProbability>

<!--
在数字1、2、3、4四个数中，任取两个不同的数，其和大于积的概率是
-->