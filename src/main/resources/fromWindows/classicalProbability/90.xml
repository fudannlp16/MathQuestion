<?xml version="1.0" encoding="utf-8"?>

<ClassicalProbability> 
  <Experiment> 
    <!--试验-->  
    <ExperimentObject> 
      <!--试验对象-->  
      <ObjectName>纸片</ObjectName>  
      <!--对象名称-->  
      <ObjectTotality>6</ObjectTotality>  
      <!--对象总数，如果未知，用n表示-->  
        <DiscreteName>0;1;2;3;4;5</DiscreteName> 
        <DomainQuantity>1;1;1;1;1;1</DomainQuantity>  <!--默认值均是1-->
    </ExperimentObject>  
     <ExperimentMethod> 
        <!--试验方法-->  
        <SamplingWay>0</SamplingWay><!--抽取方式：放回(0)与不放回(1)，默认值是1-->  
        <SamplingQuantity>1</SamplingQuantity>          <!--抽取的数量，默认值1-->  
        <SortingWay>1</SortingWay> <!--排序方式：有序(0)和无序(1)，默认值是1--> 
      </ExperimentMethod>  
    <ExperimentTimes>2</ExperimentTimes><!--试验次数，默认值为1-->
    <ExperimentResult>  
        <DiscreteName>null</DiscreteName>  
        <DomainQuantity>null</DomainQuantity>  <!--默认值均是1-->
    </ExperimentResult>     
  </Experiment>
  <Question>
    <EventName>两张卡片上数字之和为7的概率</EventName>
    <ResultFilter>和(x1,x2)=7</ResultFilter>
    <Probability>null</Probability>
  </Question>
</ClassicalProbability>

<!--
在A，B两个袋中都有6张分别写有数字0，1，2，3，4，5的卡片，现
从每个袋中任取一张卡片，则两张卡片上数字之和为7的概率为（）
-->