<?xml version="1.0" encoding="utf-8"?>

<ClassicalProbability> 
  <Experiment> 
    <!--试验-->  
    <ExperimentObject> 
      <!--试验对象-->  
      <ObjectName>编码</ObjectName>  
      <!--对象名称-->  
      <ObjectTotality>2</ObjectTotality>  
      <!--对象总数，如果未知，用n表示-->  
        <DiscreteName>0;1</DiscreteName> 
        <DomainQuantity>1;1</DomainQuantity>  <!--默认值均是1-->
    </ExperimentObject>  
     <ExperimentMethod> 
        <!--试验方法-->  
        <SamplingWay>0</SamplingWay><!--抽取方式：放回(0)与不放回(1)，默认值是1-->  
        <SamplingQuantity>1</SamplingQuantity>          <!--抽取的数量，默认值1-->  
        <SortingWay>0</SortingWay> <!--排序方式：有序(0)和无序(1)，默认值是1--> 
      </ExperimentMethod>  
    <ExperimentTimes>4</ExperimentTimes><!--试验次数，默认值为1-->
    <ExperimentResult>  
        <DiscreteName>null</DiscreteName>  
        <DomainQuantity>null</DomainQuantity>  <!--默认值均是1-->
    </ExperimentResult>     
  </Experiment>
  <Question>
    <EventName>码中至少有两个1的概率</EventName>
    <ResultFilter>至少(1)=2</ResultFilter>
    <Probability>null</Probability>
  </Question>
</ClassicalProbability>

<!--
用0，1两个数字编码，码长为4（即为二进制四位数，首位可以是0），
从所有码中任选一码，则码中至少有两个1的概率是（）
-->