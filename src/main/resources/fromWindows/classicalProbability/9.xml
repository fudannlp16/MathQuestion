<?xml version="1.0" encoding="utf-8"?>

<ClassicalProbability> 
  <Experiment> 
    <!--试验-->  
    <ExperimentObject> 
      <!--试验对象-->  
      <ObjectName>钥匙</ObjectName>  
      <!--对象名称-->  
      <ObjectTotality>5</ObjectTotality>  
      <!--对象总数，如果未知，用n表示-->  
        <DiscreteName>能开锁的钥匙;不能开锁的钥匙</DiscreteName>  
        <!--离散值域的名称-->  
        <DomainQuantity>2;null</DomainQuantity>  
        <!--离散值的数量，默认值为1-->  
    </ExperimentObject>  
      <ExperimentMethod> 
        <SamplingWay>1</SamplingWay>  <!--抽取方式：放回(0)与不放回(1)，默认值是1-->  
        <SamplingQuantity>2</SamplingQuantity>  
        <!--抽取的数量-->  
        <SortingWay>1</SortingWay>  <!--排序方式：有序(0)和无序(1)，默认值是1--> 
      </ExperimentMethod>   
    <ExperimentTimes>1</ExperimentTimes>  <!--试验次数，默认值为1-->
    <ExperimentResult> 
        <DiscreteName>null</DiscreteName>   
        <DomainQuantity>null</DomainQuantity><!--实验结果离散值的数量，默认值为1--> 
    </ExperimentResult>
  </Experiment>
  <Question>
    <EventName>从中任取2把能将该锁打开的概率</EventName>
    <ResultFilter>
    	能打开锁(x1,x2)
    </ResultFilter>
    <Probability>null</Probability>
  </Question>
</ClassicalProbability> 

