<?xml version="1.0" encoding="utf-8"?>

<ClassicalProbability> 
  <Experiment> 
    <!--试验-->  
    <ExperimentObject> 
      <!--试验对象-->  
      <ObjectName>值班</ObjectName>  
      <!--对象名称-->  
      <ObjectTotality>3</ObjectTotality>  
      <!--对象总数，如果未知，用n表示-->  
        <DiscreteName>甲;乙;丙</DiscreteName> 
        <DomainQuantity>1;1;1</DomainQuantity>  <!--默认值均是1-->
    </ExperimentObject>  
     <ExperimentMethod> 
        <!--试验方法-->  
        <SamplingWay>1</SamplingWay><!--抽取方式：放回(0)与不放回(1)，默认值是1-->  
        <SamplingQuantity>3</SamplingQuantity>          <!--抽取的数量，默认值1-->  
        <SortingWay>0</SortingWay> <!--排序方式：有序(0)和无序(1)，默认值是1--> 
      </ExperimentMethod>  
    <ExperimentTimes>1</ExperimentTimes><!--试验次数，默认值为1-->
    <ExperimentResult>  
        <DiscreteName>null</DiscreteName>  
        <DomainQuantity>null</DomainQuantity>  <!--默认值均是1-->
    </ExperimentResult>     
  </Experiment>
  <Question>
    <EventName>甲紧接着排在乙的前面值班的概率</EventName>
    <ResultFilter>紧接(前面(甲,乙))</ResultFilter>
    <Probability>null</Probability>
  </Question>
</ClassicalProbability>

<!--
甲、乙、丙三人在3天节日中值班，每人值班1天，则甲紧接着排在乙的前面值班的概率是（）
