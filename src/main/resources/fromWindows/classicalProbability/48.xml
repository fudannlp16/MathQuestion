<?xml version="1.0" encoding="utf-8"?>

<ClassicalProbability> 
  <Experiment> 
    <!--试验-->  
    <ExperimentObject> 
      <!--试验对象-->  
      <ObjectName>球</ObjectName>  
      <!--对象名称-->  
      <ObjectTotality>9</ObjectTotality>  
      <!--对象总数，如果未知，用n表示-->  
        <DiscreteName>红球;黄球</DiscreteName>  
        <!--离散值域的名称-->  
        <DomainQuantity>6;3</DomainQuantity>  
        <!--离散值的数量，默认值为1-->  
    </ExperimentObject>  
      <ExperimentMethod> 
        <SamplingWay>1</SamplingWay>  <!--抽取方式：放回(0)与不放回(1)，默认值是1-->  
        <SamplingQuantity>6</SamplingQuantity>  
        <!--抽取的数量-->  
        <SortingWay>1</SortingWay>  <!--排序方式：有序(0)和无序(1)，默认值是1--> 
      </ExperimentMethod>   
    <ExperimentTimes>1</ExperimentTimes>  <!--试验次数，默认值为1-->
    <ExperimentResult> 
        <DiscreteName>红球;黄球</DiscreteName>   
        <DomainQuantity>null</DomainQuantity><!--实验结果离散值的数量，默认值为1--> 
    </ExperimentResult>
  </Experiment>
  <Question>
    <EventName>红色球与黄色球恰好相等的概率</EventName>
    <ResultFilter>
    	相等(红色球,黄色球)
    </ResultFilter>
    <Probability>null</Probability>
  </Question>
</ClassicalProbability>

<!--
袋中有同样的球9个，其中6个红色，3个黄色，现从中随机地摸6球，求：
红色球与黄色球恰好相等的概率.
-->