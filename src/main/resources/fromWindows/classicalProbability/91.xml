<?xml version="1.0" encoding="utf-8"?>

<ClassicalProbability> 
  <Experiment> 
    <!--试验-->  
    <ExperimentObject> 
      <!--试验对象-->  
      <ObjectName>球</ObjectName>  
      <!--对象名称-->  
      <ObjectTotality>8</ObjectTotality>  
      <!--对象总数，如果未知，用n表示-->  
        <DiscreteName>1;2;3;4;5;6;7;8</DiscreteName> 
        <DomainQuantity>1;1;1;1;1;1;1;1</DomainQuantity>  <!--默认值均是1-->
    </ExperimentObject>  
     <ExperimentMethod> 
        <!--试验方法-->  
        <SamplingWay>0</SamplingWay><!--抽取方式：放回(0)与不放回(1)，默认值是1-->  
        <SamplingQuantity>1</SamplingQuantity>          <!--抽取的数量，默认值1-->  
        <SortingWay>1</SortingWay> <!--排序方式：有序(0)和无序(1)，默认值是1--> 
      </ExperimentMethod>  
    <ExperimentTimes>2</ExperimentTimes><!--试验次数，默认值为1-->
    <ExperimentResult>  
        <DiscreteName>null</DiscreteName>  
        <DomainQuantity>null</DomainQuantity>  <!--默认值均是1-->
    </ExperimentResult>     
  </Experiment>
  <Question>
    <EventName>取得两球的编号和不小于15的概率</EventName>
    <ResultFilter>不小于(和(x1+x2),15)</ResultFilter>
    <Probability>null</Probability>
  </Question>
</ClassicalProbability>

<!--
一袋中装有大小相同，编号分别为1，2，3，4，5，6，7，8的八个球，从中有放回地每次取一个球，共取2次，则取得两球的编号和不小于15的概率为（ ）
-->