<?xml version="1.0" encoding="utf-8"?>

<ClassicalProbability> 
  <Experiment> 
    <!--试验-->  
    <ExperimentObject> 
      <!--试验对象-->  
      <ObjectName>饮料</ObjectName>  
      <!--对象名称-->  
      <ObjectTotality>6</ObjectTotality>  
      <!--对象总数，如果未知，用n表示-->  
        <DiscreteName>未过保质期;过保质期</DiscreteName>  
        <!--离散值域的名称-->  
        <DomainQuantity>null;2</DomainQuantity>  
        <!--离散值的数量，默认值为1-->  
    </ExperimentObject>  
      <ExperimentMethod> 
        <SamplingWay>1</SamplingWay>  <!--抽取方式：放回(0)与不放回(1)，默认值是1-->  
        <SamplingQuantity>2</SamplingQuantity>  
        <!--抽取的数量-->  
        <SortingWay>1</SortingWay>  <!--排序方式：有序(0)和无序(1)，默认值是1--> 
      </ExperimentMethod>   
    <ExperimentTimes>1</ExperimentTimes>  <!--试验次数，默认值为1-->
    <ExperimentResult> 
        <DiscreteName>null</DiscreteName>   
        <DomainQuantity>null</DomainQuantity><!--实验结果离散值的数量，默认值为1--> 
    </ExperimentResult>
  </Experiment>
  <Question>
    <EventName>至少取到一瓶已过保质期饮料概率</EventName>
    <ResultFilter>
    	至少(已过保质期)=1
    </ResultFilter>
    <Probability>null</Probability>
  </Question>
</ClassicalProbability>

