<?xml version="1.0" encoding="utf-8"?>

<ClassicalProbability> 
  <Experiment> 
    <!--试验-->  
    <ExperimentObject> 
      <!--试验对象-->  
      <ObjectName>同学</ObjectName>  
      <!--对象名称-->  
      <ObjectTotality>40</ObjectTotality>  
      <!--对象总数，如果未知，用n表示-->  
        <DiscreteName>男生;女生</DiscreteName>  
        <!--离散值域的名称-->  
        <DomainQuantity>25;15</DomainQuantity>  
        <!--离散值的数量，默认值为1-->  
    </ExperimentObject>  
      <ExperimentMethod> 
        <SamplingWay>1</SamplingWay>  <!--抽取方式：放回(0)与不放回(1)，默认值是1-->  
        <SamplingQuantity>5</SamplingQuantity>  
        <!--抽取的数量-->  
        <SortingWay>1</SortingWay>  <!--排序方式：有序(0)和无序(1)，默认值是1--> 
      </ExperimentMethod>   
    <ExperimentTimes>1</ExperimentTimes>  <!--试验次数，默认值为1-->
    <ExperimentResult> 
        <DiscreteName>null</DiscreteName>   
        <DomainQuantity>null</DomainQuantity><!--实验结果离散值的数量，默认值为1--> 
    </ExperimentResult>
  </Experiment>
  <Question>
    <EventName>至少有2名女班委的概率</EventName>
    <ResultFilter>
    	至少(女生)=2
    </ResultFilter>
    <Probability>null</Probability>
  </Question>
</ClassicalProbability>

<!--
某班有学生40人,其中男生25人,女生15人,任选5人组成班委会,则至少有2名女班委的概率是( )
-->