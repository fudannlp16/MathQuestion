<?xml version="1.0" encoding="utf-8"?>
<Stratification> 
  <TS> <!--TotalSet表示总体-->
    <name>总数</name>  <!--默认值是“总体”-->
    <quantity>null</quantity>  <!--默认值是“null”-->
    <unit>null</unit>	<!--默认值是“null”-->
  </TS>  
  <SOT>  <!--Stratification of TotalSet表示总体中的层-->
    <name>A</name>   
    <quantity>null</quantity>  
    <unit>null</unit> 
  </SOT>  
  <SOT> 
    <name>B</name>   
    <quantity>null</quantity>  
    <unit>null</unit> 
  </SOT>  
  <SOT> 
    <name>C</name>   
    <quantity>null</quantity>  
    <unit>null</unit> 
  </SOT>  
  <S> <!--Sample表示样本-->
    <name>样本</name>  <!--默认值是“样本”-->
    <quantity>100</quantity>  
    <unit>null</unit> 
  </S>  
  <SOS> <!--Stratification of Sample表示样本中的层-->
    <name>A</name>   
    <quantity>null</quantity>  
    <unit>null</unit>
  </SOS>  
  <SOS> 
    <name>B</name>   
    <quantity>null</quantity>  
    <unit>null</unit> 
  </SOS>
  <SOS> 
    <name>C</name>   
    <quantity>null</quantity>  
    <unit>null</unit> 
  </SOS>  
  <EQ> <!--关系表征  B/C=10/1-->
    <entityName>A,B,C</entityName>
    <relation>比</relation>
    <value>5:3:2</value>
  </EQ>
  <QUE>
    <description>应从C中抽取</description>
    <belong>样本</belong>
    <name>C</name>
    <unit>null</unit>
  </QUE>
</Stratification>

<!--
将一个总数分为A、B、C三层，其个体数之比为5：3：2。若用分层抽样方法抽取容量为100的样本，则应从C中抽取（    ）个个体。
-->